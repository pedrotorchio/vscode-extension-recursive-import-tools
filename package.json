{
  "name": "import-recursive-search",
  "displayName": "Import recursive search",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "viewsWelcome": [
      {
        "view": "imports-tree",
        "contents": "No file selected. Please select a file to see its imports."
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "imports-tree",
          "name": "Imports tree"
        }
      ]
    },
    "commands": [
      {
        "command": "import-recursive-search.search",
        "title": "Search imports recursively"
      }
    ],
    "activationEvents": [
      "onView:imports-tree"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "find-package-json": "^1.2.0",
    "resolve": "^1.22.10",
    "typescript-parser": "^2.6.1"
  }
}
